{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": [2, "function"]
    },
    "globals": {
    "angular": true
  },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
